{{!-- Navigation template --}}
<ul class="u-unstyled u-flex1">
    {{#foreach navigation}}
        <li class="mapache-tracking">
            <a href="{{url absolute="true"}}" class="navigation-link u-fontSize15 u-fontWeightSemibold u-block {{#if current}}active{{/if}}">{{label}}</a>
        </li>
    {{/foreach}}
</ul>
